{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cmake",
			"label": "CMake: Configure Current Preset",
			"command": "configure",
			"preset": "${command:cmake.activeConfigurePresetName}",
			"problemMatcher": [],
			"detail": "CMake template configure task"
		},
		{
			"type": "cmake",
			"label": "CMake: Build Current Preset",
			"command": "build",
			"targets": [
				"all"
			],
			"preset": "${command:cmake.activeBuildPresetName}",
			"group": "build",
			"problemMatcher": [],
			"detail": "CMake template build task"
		},
		{
			"type": "cmake",
			"label": "CMake: Install Current Preset",
			"command": "install",
			"preset": "${command:cmake.activeBuildPresetName}",
			"problemMatcher": [],
			"detail": "CMake template install task"
		}
	]
}